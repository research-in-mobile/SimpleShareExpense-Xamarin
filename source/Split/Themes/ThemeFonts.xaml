<ResourceDictionary xmlns="http://xamarin.com/schemas/2014/forms"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                    x:Class="Split.Themes.ThemeFonts">

    <OnPlatform x:TypeArguments="x:String"
                x:Key="FontAwesomeBrands">
        <On Platform="Android"
            Value="Fonts/FontAwesome5Brands.otf#Regular" />
        <On Platform="iOS"
            Value="FontAwesome5Brands-Regular" />
    </OnPlatform>

    <OnPlatform x:TypeArguments="x:String"
                x:Key="FontAwesomeSolid">
        <On Platform="Android"
            Value="Fonts/FontAwesome5Solid.otf#Regular" />
        <On Platform="iOS"
            Value="FontAwesome5Free-Solid" />
    </OnPlatform>

    <OnPlatform x:TypeArguments="x:String"
                x:Key="FontAwesomeRegular">
        <On Platform="Android"
            Value="Fonts/FontAwesome5Regular.otf#Regular" />
        <On Platform="iOS"
            Value="FontAwesome5Free-Regular" />
    </OnPlatform>
</ResourceDictionary>